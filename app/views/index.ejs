<!-- public/index.html -->
<!DOCTYPE html>
<html ng-app="contatooh">
<head>
	<meta charset="UTF-8">
	<title>Contatooh</title>
	<meta name="viewport" content="width=device-width">
	<!-- build:css css/index.min.css -->
		<link rel="stylesheet" href="vendor/bootstrap/dist/css/bootstrap.css">
		<link rel="stylesheet" href="vendor/bootstrap/dist/css/bootstrap-theme.css">
	<!-- endbuild -->
</head>
<body class="container">
	<%if (usuarioLogado.length > 0) { %>
	<div class="text-right">
		<p>
			Logado como: <span id="usuario-logado">
				<%=usuarioLogado %>
			<span>
			<a href="/logout">Logout</a>
		</p>
	</div>
	<% } %>
	<div ng-view></div>
	<!-- build:js js/index.min.js -->
		<script src="vendor/angular/angular.js"></script>
		<!-- Importando os plugins do angular. -->
		<!-- Sistema de rotas. -->
		<script src="vendor/angular-route/angular-route.js"></script>
		<!-- Sistema para consumir REST Endpoints, mais alto nivel que o '$http'. -->
		<script src="vendor/angular-resource/angular-resource.js"></script>
		<script src="vendor/angular-i18n/angular-locale_pt-br.js">
		<!-- Importando o mÃ³dulo da aplicacao. -->
		<script src="js/main.js"></script>
		<!-- Importando os controllers. -->
		<script src="js/controllers/ContatosController.js"></script>
		<script src="js/controllers/ContatoController.js"></script>
		<!-- Importando os servicos. -->
		<script src="js/services/ContatoService.js"></script>
		<script src="js/directives/meus-componentes/meus-componentes.js"></script>
	<!-- endbuild -->
</body>
</html>
